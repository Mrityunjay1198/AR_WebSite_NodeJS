<% if(users.length > 0){ %>

<div class="container">
<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">Sr no.</th>
        <th scope="col">Name</th>
        <th scope="col">Email</th>
        <th scope="col">Date</th>
      </tr>
    </thead>
    <tbody>
    <% for(var i = 0; i < users.length; i++) { %>
      <tr class="table-active">
        <th scope="row"><%= i+1 %></th>
        <td><%= users[i].name %></td>
        <td><%= users[i].email %></td>
        <td><%= users[i].password %></td>
      </tr>  
      <% } %>   
    </tbody>
  </table> 
</div>
<% } %>